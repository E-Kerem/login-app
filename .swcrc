{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": true, // Enable TSX syntax support (for TypeScript projects)
        "decorators": true
      },
      "transform": {
        "react": {
          "pragma": "React.createElement",
          "pragmaFrag": "React.Fragment",
          "throwIfNamespace": true,
          "development": false,
          "useBuiltins": false
        }
      },
      "target": "es2019"
    },
    "module": {
      "type": "commonjs"
    }
  }
  